/////////////////////////////////////////////////
// SCHEDULING
/////////////////////////////////////////////////

job_name = "demo-nginx"

/////////////////////////////////////////////////
// GROUP
/////////////////////////////////////////////////

ports = [{
  label  = "http"
  to     = 80
  static = -1
}]

/////////////////////////////////////////////////
// CONSUL
/////////////////////////////////////////////////

consul_service = {
  name = "demo-nginx"
  port = "80"
  connect = true
}

consul_tags = ["traefik.enable=false"]
consul_meta = {
  metrics_prometheus_enabled = "false"
}

/////////////////////////////////////////////////
// TASK nginx
/////////////////////////////////////////////////
# N/A
